{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "no-console": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "comma-dangle": ["error", "never"],
      "no-underscore-dangle": ["off"],
      "prop-types": ["off"],
      "linebreak-style": "off",
      "import/no-extraneous-dependencies": "off"
    }
}